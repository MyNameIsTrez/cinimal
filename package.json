{
  "name": "cinimal",
  "publisher": "MyNameIsTrez",
  "displayName": "cinimal",
  "description": "Edit C files through the lens of Python-like syntax.",
  "version": "0.1.0",
  "repository": {
    "url": "https://github.com/MyNameIsTrez/cinimal"
  },
  "icon": "./vsicons-custom-icons/file_type_cn.png",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "cinimal",
        "aliases": [
          "cinimal"
        ],
        "extensions": [
          ".cn"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cinimal",
        "scopeName": "source.cn",
        "path": "./syntaxes/cinimal.tmLanguage.json"
      }
    ],
    "configuration": [
      {
        "title": "cinimal",
        "properties":{
          "cinimal.pythonIndentation": {
            "type": "boolean",
            "scope": "resource",
            "default": true,
            "description": "Use Python-like tab indentation."
          }
        }
      }
    ]
  }
}
