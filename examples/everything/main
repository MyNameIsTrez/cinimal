import { bark, get_paw_count } from animals/dog.t

struct names
	str first_name
	str last_name

enum country
	ukraine
	mexico

struct person
	i8 age
	names names
	country country

i32	main()
	i32 i
	i = 0
	while i < 3
		print(i)
		i += 1

	dog::bark()

	i16 paw_count
	paw_count = dog::get_paw_count(2)

	ptr i16 paw_count_ptr
	paw_count_ptr = &paw_count
	assert(*paw_count_ptr, 8)

	ptr person people
	people = malloc(2, person)

	ptr person frank
	frank = &people[0]
	frank->age = 24
	assert(people[0]->age, 24)
	frank->names.last_name = "frank"

	assert(people[1].names.last_name, "")
	people[1].country = country::ukraine
	nassert(people[1].country, country::mexico)

	free(people)

	return 0
